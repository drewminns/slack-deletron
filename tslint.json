{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "new-parens": true,
    "no-bitwise": true,
    "no-empty-interface": true,
    "curly": true,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-shadowed-variable": [
      true,
      {
        "class": true,
        "enum": true,
        "function": true,
        "interface": false,
        "namespace": true,
        "typeAlias": false,
        "typeParameter": false,
        "underscore": false
      }
    ],
    "radix": true,
    "triple-equals": true,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "class-name": true,
    "prefer-template": true,
    "eofline": true,
    "no-trailing-whitespace": true,
    "semicolon": [true, "always", "ignore-interfaces"],
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "rulesDirectory": []
}
